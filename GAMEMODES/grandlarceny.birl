//OnPlayerUpdate
AUUU FICA DE OLHO NO MONSTRO(MONSTRAO) BORA
	
	ELE QUE A GENTE QUER?(!O MUTANTE TA AI?(MONSTRAO)) BORA
		VAMO CUMPADE 0;
	BIRL	

	ELE QUE A GENTE QUER?(E MONSTRO?(MONSTRAO)) BORA
		VAMO CUMPADE 1;
	BIRL

	ELE QUE A GENTE QUER?(!gPlayerHasCitySelected[MONSTRAO]  &&  COMO ELE TA?(MONSTRAO) == SO OLHANDO NE PARCEIRO) BORA
		ClassSel_HandleCitySelection(MONSTRAO);
		VAMO CUMPADE 1;
	BIRL

	ELE QUE A GENTE QUER?(O QUE ELE TA SEGURANDO?(MONSTRAO) == WEAPON_MINIGUN) BORA
		SAI DAQUI RAPA!(MONSTRAO);
		VAMO CUMPADE 0;
	BIRL

	VAMO CUMPADE 1;
BIRL
